<?xml version="1.0" encoding="utf-8"?>
 <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
     android:id="@+id/incomingevent"
     android:layout_width="match_parent"
     android:layout_height="wrap_content" >
     <TextView
         android:id="@+id/incevent1"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignParentTop="true"
         android:layout_centerHorizontal="true"
         android:layout_marginTop="31dp"
         android:text="Inkommande larm!"
         android:textAppearance="?android:attr/textAppearanceLarge" />
     
     <TextView
         android:id="@+id/incevent"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_below="@+id/incevent1"
         android:layout_centerHorizontal="true"
         android:layout_marginTop="50dp"
         android:text="Du har"
         android:textAppearance="?android:attr/textAppearanceMedium" />

     <TextView
         android:id="@+id/incevent2"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_below="@+id/incevent"
         android:layout_centerHorizontal="true"
         android:text=""
         android:textAppearance="?android:attr/textAppearanceLarge" />

     <TextView
         android:id="@+id/incevent3"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_below="@+id/incevent2"
         android:layout_centerHorizontal="true"
         android:text="sekunder pÃ¥ dig att svara."
         android:textAppearance="?android:attr/textAppearanceMedium" />

     <Button
         android:id="@+id/acceptbtn"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"         
         android:layout_centerHorizontal="true"
         android:layout_below="@+id/incevent3"
         android:layout_marginTop="20dp"
         android:layout_weight="1"
         android:text="Kvittera" />

 </RelativeLayout>